    "tokenColors": [
        {
            "name": "Keywords and Build-ins",
            "scope": [
                "meta.function storage.type",
                "meta.var.expr storage.type",
                "meta.class storage.type",
                "storage.modifier",
                "new.expr entity.name.function",
                "keyword",
                "support.class.builtin",
                "constant.language.boolean",
                "constant.language.null",
                "storage.type.primitive"
            ],
            "settings": {
                "foreground": "#FFE074",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Keywords and Build-ins :: override",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#FFE074",
                "fontStyle": ""
            }
        },
        {
            "name": "Keywords and Build-ins :: override",
            "scope": [
                "variable.language.this"
            ],
            "settings": {
                "foreground": "#8BC0FF",
                "fontStyle": ""
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "meta.function meta.brace.round"
            ],
            "settings": {
                "foreground": "#45CAC2"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable.other.object.property",
                "meta.block variable.other.property"
            ],
            "settings": {
                "foreground": "#8BC0FF"
            }
        },
        {
            "name": "Variables :: this",
            "scope": [
                "variable.language.this"
            ],
            "settings": {
                "foreground": "#8BC0FF",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Variables :: props",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#8BC0FF",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Class",
            "scope": [
                "support.class",
                "meta.class entity.name.type.class",
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#8BC0FF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Class :: super",
            "scope": [
                "variable.language.super"
            ],
            "settings": {
                "foreground": "#45CAC2",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Objects",
            "scope": [
                "meta.object.member entity.name.function"
            ],
            "settings": {
                "foreground": "#45CAC2"
            }
        },
        {
            "name": "Objects :: overrides",
            "scope": [
                "meta.method punctuation.definition.parameters.begin",
                "meta.method punctuation.definition.parameters.end"
            ],
            "settings": {
                "foreground": "##45CAC2"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string.quoted"
            ],
            "settings": {
                "foreground": "#FC9292"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#F7C0A5"
            }
        },
        {
            "name": "Imports",
            "scope": [
                "variable.other.readwrite.alias"
            ],
            "settings": {
                "foreground": "#8BC0FF"
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#414B5D"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#7E8694"
            }
        },
        {
            "name": "[Vue] After first level properties",
            "scope": [
                "source.vue meta.object.member meta.object.member meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#BAB4FF"
            }
        },
        {
            "name": "[XML] Tags",
            "scope": [
                "entity.name.tag",
                "meta.tag",
                "entity.name.tag.pug"
            ],
            "settings": {
                "foreground": "#FFCC6A"
            }
        },
        {
            "name": "[XML] Tags :: strings",
            "scope": [
                "text.html.derivative",
                "meta.jsx.children"
            ],
            "settings": {
                "foreground": "#F7F7E5"
            }
        },
        {
            "name": "[XML] Tags :: directives",
            "scope": [
                "entity.other.attribute-name",
                "meta.directive"
            ],
            "settings": {
                "foreground": "#FFB681"
            }
        },
        {
            "name": "[XML] Tags :: custom components",
            "scope": [
                "meta.tag.other entity.name.tag",
                "support.class.component"
            ],
            "settings": {
                "foreground": "#BAB4FF"
            }
        },
        {
            "name": "[XML] Tags :: variables",
            "scope": [
                "meta.tag variable"
            ],
            "settings": {
                "foreground": "#8BC0FF"
            }
        },
        {
            "name": "[CSS] Constants",
            "scope": [
                "support.constant"
            ],
            "settings": {
                "foreground": "#FC9292"
            }
        },
        {
            "name": "[CSS] Constants :: Colors",
            "scope": [
                "constant.other",
                "support.constant.color"
            ],
            "settings": {
                "foreground": "#FFE074",
                "fontStyle": "italic"
            }
        },
        {
            "name": "[JSON] Key",
            "scope": [
                "meta.structure.dictionary"
            ],
            "settings": {
                "foreground": "#F7F7E5"
            }
        },
        {
            "name": "[JSON] Value",
            "scope": [
                "meta.structure.dictionary.value"
            ],
            "settings": {
                "foreground": "#FFE074"
            }
        },
        {
            "name": "[Markdown] Title",
            "scope": [
                "markup.heading"
            ],
            "settings": {
                "foreground": "#FC9292"
            }
        },
        {
            "name": "[Markdown] Punctuation",
            "scope": [
                "markup.heading.setext"
            ],
            "settings": {
                "foreground": "#7E8694"
            }
        },
        {
            "name": "[Markdown] Quote",
            "scope": [
                "markup.quote"
            ],
            "settings": {
                "foreground": "#DA95EF"
            }
        },
        {
            "name": "[Markdown] Quote",
            "scope": [
                "meta.image.inline"
            ],
            "settings": {
                "foreground": "#FFE074"
            }
        },
        {
            "name": "[Markdown] Link",
            "scope": [
                "meta.link.inline"
            ],
            "settings": {
                "foreground": "#FFCC6A"
            }
        },
        {
            "name": "[Markdown] Bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "[Markdown] Italic",
            "scope": [
                "markup.italic.markdown"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "variable.defaultLibrary": "#8BC0FF",
        "variable": "#8BC0FF",
        "function": "#45CAC2"
    }
}