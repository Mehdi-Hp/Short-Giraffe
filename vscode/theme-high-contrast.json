   "tokenColors": [
      {
         "name": "Punctuation",
         "scope": [
            "punctuation",
            "meta.brace"
         ],
         "settings": {
            "foreground": "#A3A5AA"
         }
      },
      {
         "name": "Keywords and Built-ins",
         "scope": [
            "meta.class storage.type",
            "storage.modifier",
            "storage.type.type",
            "support.type.primitive",
            "new.expr entity.name.function",
            "meta.var.expr storage",
            "variable.language.this",
            "keyword",
            "meta.function storage.type.function",
            "constant.language.boolean",
            "constant.language.null",
            "storage.type.primitive",
            "support.type.object.module"
         ],
         "settings": {
            "foreground": "#FFE074",
            "fontStyle": "italic"
         }
      },
      {
         "name": "Built-in objects",
         "scope": [
            "support.class.builtin"
         ],
         "settings": {
            "foreground": "#FFB46A",
            "fontStyle": "bold"
         }
      },
      {
         "name": "Keywords and Build-ins :: override",
         "scope": [
            "keyword.operator"
         ],
         "settings": {
            "foreground": "#FFE074",
            "fontStyle": "normal"
         }
      },
      {
         "name": "Types",
         "scope": [
            "entity.name.type"
         ],
         "settings": {
            "foreground": "#cfbafa"
         }
      },
      {
         "name": "Functions",
         "scope": [
            "entity.name.function"
         ],
         "settings": {
            "foreground": "#5ccfe6"
         }
      },
      {
         "name": "Variables",
         "scope": [
            "variable"
         ],
         "settings": {
            "foreground": "#9be7ff",
            "fontStyle": "normal"
         }
      },
      {
         "name": "Variables :: props",
         "scope": [
            "variable.parameter"
         ],
         "settings": {
            "foreground": "#6dcbfa",
            "fontStyle": "italic"
         }
      },
      {
         "name": "Class",
         "scope": [
            "meta.class support.class",
            "meta.class entity.name.type.class",
            "entity.other.inherited-class"
         ],
         "settings": {
