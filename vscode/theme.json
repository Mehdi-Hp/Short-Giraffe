   "tokenColors": [
      {
         "name": "Punctuation",
         "scope": [
            "punctuation",
            "meta.brace"
         ],
         "settings": {
            "foreground": "#7581A0"
         }
      },
      {
         "name": "Keywords and Built-ins",
         "scope": [
            "meta.class storage.type",
            "storage.modifier",
            "storage.type.type",
            "support.type.primitive",
            "new.expr entity.name.function",
            "meta.var.expr storage",
            "variable.language.this",
            "keyword",
            "meta.function storage.type.function",
            "constant.language.boolean",
            "constant.language.null",
            "storage.type.primitive",
            "support.type.object.module"
         ],
         "settings": {
            "foreground": "#F5DA7A",
            "fontStyle": "italic"
         }
      },
      {
         "name": "Built-in objects",
         "scope": [
            "support.class.builtin"
         ],
         "settings": {
            "foreground": "#FFB46A",
            "fontStyle": "bold"
         }
      },
      {
         "name": "Keywords and Build-ins :: override",
         "scope": [
            "keyword.operator"
         ],
         "settings": {
            "foreground": "#FFE074",
            "fontStyle": "normal"
         }
      },
      {
         "name": "Types",
         "scope": [
            "entity.name.type"
         ],
         "settings": {
            "foreground": "#9be7ff"
         }
      },
      {
         "name": "Functions",
         "scope": [
            "entity.name.function"
         ],
         "settings": {
            "foreground": "#45CAC2"
         }
      },
      {
         "name": "Variables",
         "scope": [
            "variable"
         ],
         "settings": {
            "foreground": "#BBD2EE",
            "fontStyle": "normal"
         }
      },
      {
         "name": "Variables :: props",
         "scope": [
            "variable.parameter"
         ],
         "settings": {
            "foreground": "#9BC9FF",
            "fontStyle": "italic"
         }
      },
      {
         "name": "Class",
         "scope": [
            "meta.class support.class",
            "meta.class entity.name.type.class",
            "entity.other.inherited-class"
         ],
         "settings": {
            "foreground": "#9BC9FF",
            "fontStyle": "bold"
         }
      },
      {
         "name": "Class :: super",
         "scope": [
            "variable.language.super"
         ],
         "settings": {
            "foreground": "#45CAC2",
            "fontStyle": "bold"
         }
      },
      {
         "name": "Objects/Keys",
         "scope": [
            "meta.object-literal.key"
         ],
         "settings": {
            "foreground": "#FCFCCE"
         }
      },
      {
         "name": "Objects/Methods",
         "scope": [
            "meta.object.member function",
            "meta.object.member meta.function-call",
            "meta.brace.round meta.object.member"
         ],
         "settings": {
            "foreground": "#45CAC2"
         }
      },
      {
         "name": "Objects/Properties",
         "scope": [
            "meta.object.member variable"
         ],
         "settings": {
            "foreground": "#FCFCCE"
         }
      },
      {
         "name": "Strings",
         "scope": [
            "string"
         ],
         "settings": {
            "foreground": "#F6ABA8"
         }
      },
      {
         "name": "Numbers",
         "scope": [
            "constant.numeric"
         ],
         "settings": {
            "foreground": "#F7C0A5"
         }
      },
      {
         "name": "Imports",
         "scope": [
            "meta.import variable.other.readwrite"
         ],
         "settings": {
            "foreground": "#9BC9FF"
         }
      },
      {
         "name": "Comments",
         "scope": [
            "comment"
