            "source.vue support.class"
         ],
         "settings": {
            "foreground": "#BAB4FF"
         }
      },
      {
         "name": "[XML] Tags",
         "scope": [
            "entity.name.tag",
            "meta.tag",
            "entity.name.tag.pug"
